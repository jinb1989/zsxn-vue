mmit(\"increment\",count);\r\n                            Toast(result.body.msg);\r\n                        }else{\r\n                            Toast(result.body.msg);\r\n                        }\r\n                    });\r\n                //2.更新app.vue组件购物车数量\r\n\r\n            }\r\n        },\r\n        created(){\r\n            this.getImageList();\r\n            this.getGoodInfo();\r\n            //console.log(this.$route.query.id);\r\n             console.log(this.$route.params.id);//以/传参时接收参数的写法\r\n        },components:{//注册\r\n            \"swiper-box\":swiper\r\n        }\r\n    }\r\n</script>\r\n<style>\r\n.desc_image{\r\n    width:100%;\r\n    height:200px;\r\n}\r\n    .detailcontent {\r\n    width: 100%;\r\n    height: auto !important;\r\n    height: 400px;\r\n    overflow: visible;\r\n    margin: 0px auto;\r\n}\r\n.detailparam {\r\n    width: 100%;\r\n    height: auto;\r\n    overflow: hidden;\r\n    margin: 0px auto;\r\n}\r\n.detailparam .name {\r\n    width: 100%;\r\n    overflow: hidden;\r\n    text-indent: 28px;\r\n    font-family: \"Microsoft YaHei\" !important;\r\n    font-size: 18px;\r\n    color: #333;\r\n}\r\n.detailparam .extend {\r\n    width: 100%;\r\n    overflow: hidden;\r\n    background: #fafafa;\r\n}\r\n.detailparam .extend ul {\r\n   /* margin-top: 20px;*/\r\n    list-style: none;\r\n}\r\n.detailparam .extend ul li {\r\n    float: left;\r\n    width: 223px;\r\n    height: 50px;\r\n    overflow: hidden;\r\n    line-height: 50px;\r\n    border-bottom: 1px dashed #e0e0e0;\r\n}\r\n.detailparam .extend ul li em{\r\n    font-size:14px;\r\n\tfont-family: \"Microsoft YaHei\" !important;\r\n\tcolor:#333;\r\n\tfont-style:normal;\r\n}\r\n.detailparam .extend ul li span{\r\n\tfont-size:12px;\r\n\tcolor:#000;\r\n}\r\n</style>"],"sourceRoot":"src\\components\\goods"}]}