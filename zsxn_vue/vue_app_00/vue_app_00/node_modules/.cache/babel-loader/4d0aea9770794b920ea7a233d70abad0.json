ge\" @click=\"postComment\">发表评论</mt-button>\r\n        <!--显示评论区域-->\r\n        <div class=\"cmt-list\">\r\n            <div class=\"cmt-item\" v-for=\"(item,i) in list \" :key=\"item.id\">\r\n                <div class=\"cmt-info\">\r\n                第{{i+1}}楼 用户名:{{item.user_name}} \r\n                发表时间:{{item.ctime | dateFilter}}\r\n                </div>\r\n                <div class=\"cmt-body\">{{item.content}}</div>\r\n            </div>\r\n        </div>\r\n        <mt-button size=\"large\" @click=\"getMore\">加载更多</mt-button>\r\n    </div>\r\n</template>\r\n<script>\r\n//单独引入一个组件\r\nimport {Toast} from 'mint-ui'\r\n    export default {\r\n        data(){\r\n            return {\r\n                list:[],        //分页数据\r\n                msg:\"\",         //双向绑定的用户评论\r\n                pageIndex:0,    //当前页\r\n                pageSize:5,     //页大小\r\n            }\r\n        },  \r\n        created(){\r\n            this.getMore();\r\n        },\r\n        methods:{\r\n            getMore(){\r\n                //1.页码自增\r\n                this.pageIndex++;\r\n                //2.发送ajax  /getComment\r\n                var p=this.pageIndex;  //页码\r\n                var s=this.pageSize;     //页大小\r\n                var id=this.id;          //新闻编号\r\n                var url=\"getComment?id=\"+id+\"&pno=\"+p+\"&pageSize=\"+s;                \r\n                //3.参数  id  pno  pageSize\r\n                //4.获取返回数据保存list属性\r\n                this.$http.get(url).then(result=>{\r\n                    //console.log(result)\r\n                    var row=this.list.concat(result.body.data);\r\n                    this.list=row;\r\n                })\r\n                //5.显示模板中 \r\n            },\r\n            postComment(){\r\n                //1.获取新闻编号\r\n                var nid=this.id;\r\n                //2.获取评论内容\r\n                var pmsg=this.msg;\r\n                //3.判断新闻评论内容不能为空\r\n                if(pmsg.trim().length==0){\r\n                    Toast(\"评论内容不能为空\")\r\n                    return;\r\n                }\r\n                //4.发送post请求\r\n                var url=\"http://127.0.0.1:3000/postcomment\";\r\n                this.$http.post(url,{nid:nid,msg:pmsg}).then(result=>{\r\n                Toast(\"发表成功\");\r\n                this.pageIndex=0;\r\n                this.list=[];   \r\n                this.getMore();\r\n                })\r\n                //5.获取服务器程序返回\r\n                //6.提示用户\"评论成功\"    \r\n            }\r\n        },\r\n        props:[\r\n            \"id\"\r\n        ]\r\n    }\r\n</script>\r\n<style>\r\n    .app-comment h1{\r\n        font-size:18px;\r\n    }\r\n    .app-comment textarea{\r\n        font-size:14px;\r\n        height:85px;\r\n        margin:0;\r\n    }\r\n    .app-comment .cmt-list{\r\n        margin:5px 0;\r\n    }\r\n    .app-comment .cmt-list .cmt-info{\r\n        line-height:30px;\r\n        background:#e8e8e8; \r\n    }\r\n     .app-comment .cmt-list .cmt-body{\r\n        line-height:30px;\r\n        text-indent:2em;\r\n     }\r\n</style>"],"sourceRoot":"src\\components\\sub"}]}